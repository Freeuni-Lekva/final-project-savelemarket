  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SendRequestServlet</servlet-name>
    <url-pattern>/send-request</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ManageNotificationsServlet</servlet-name>
    <url-pattern>/manage-notifications</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NotificationsServlet</servlet-name>
    <url-pattern>/notifications</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MessagesServlet</servlet-name>
    <url-pattern>/messages</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MakePostsServlet</servlet-name>
    <url-pattern>/make-posts</url-pattern>
  </servlet-mapping>

</web-app>
