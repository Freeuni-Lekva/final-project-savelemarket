<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>indexjsp</servlet-name>
    <jsp-file>/WEB-INF/index.jsp</jsp-file>
  </servlet>


  <servlet>
    <servlet-name>chat-servlet</servlet-name>
    <servlet-class>web.ChatServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>registration-servlet</servlet-name>
    <servlet-class>web.RegistrationServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>login-servlet</servlet-name>
    <servlet-class>web.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>profile-servlet</servlet-name>
    <servlet-class>web.ProfileServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ajax-servlet</servlet-name>
    <servlet-class>web.AjaxServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>show-ajax-servlet</servlet-name>
    <servlet-class>web.ShowMoreServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>create-post-servlet</servlet-name>
    <servlet-class>web.CreatePostServlet</servlet-class>
  </servlet>

  <servlet>
      <servlet-name>PrivateChatServlet</servlet-name>
      <servlet-class>web.PrivateChatServlet</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>LogOutServlet</servlet-name>
    <servlet-class>web.LogOutServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>PostDeleteServlet</servlet-name>
    <servlet-class>web.PostDeleteServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>PostsFilterServlet</servlet-name>
    <servlet-class>web.PostsFilterServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>web.HomeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SendRequestServlet</servlet-name>
    <servlet-class>web.SendRequestServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>NotificationsServlet</servlet-name>
    <servlet-class>web.NotificationsServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>MessagesServlet</servlet-name>
    <servlet-class>web.MessagesServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ManageNotificationsServlet</servlet-name>
    <servlet-class>web.ManageNotificationsServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>MakePostsServlet</servlet-name>
    <servlet-class>web.MakePostsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PrivateChatServlet</servlet-name>
    <url-pattern>/pchat</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>profile-servlet</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>indexjsp</servlet-name>
    <url-pattern>/index.jsp</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>login-servlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registration-servlet</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>chat-servlet</servlet-name>
    <url-pattern>/chat</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ajax-servlet</servlet-name>
    <url-pattern>/AjaxServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>show-ajax-servlet</servlet-name>
    <url-pattern>/ShowMoreServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>create-post-servlet</servlet-name>
    <url-pattern>/createPost</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LogOutServlet</servlet-name>
